        regs[i].name = "%xmm15"; regs[i++].size = 16;

        regs[i].name = NULL;
        regs[i].size = 0;
        regs_init = 1;
    }
    /* look for register */
    i = 0;
    while (regs[i].name && strcmp(regs[i].name, reg_name)) {
        i++;
