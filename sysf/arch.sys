        regs[i].name =   "%mm7"; regs[i++].size = 8;

        regs[i].name = "%xmm00"; regs[i++].size = 16;
        regs[i].name = "%xmm01"; regs[i++].size = 16;
        regs[i].name = "%xmm02"; regs[i++].size = 16;
